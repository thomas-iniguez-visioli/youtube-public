{
  "name": "youtube",
  "version": "1.1.37-11",
  "description": "youtube local",
  "main": "src/index.js",
  "private": true,

  
  "scripts": {
    "build": "bun i helmet cors && bun x electron-rebuild -y  ",
    "prepush": "bun test",
    "start": "electron .",
    "test": "bun test tests/*.test.js"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "ejs": "^3.1.10",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.3.9",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "rollbar": "^2.26.5",
    "socket.io": "^4.4.1",
    "unzipper": "^0.12.3",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "electron",
    "video",
    "streaming",
    "node"
  ],
  "author": "arbinger",
  "license": "MIT",
  "build": {
    "appId": "com.arbinger.youtube",
    "win": {
      "target": [
        "NSIS",
        "portable"
      ],
      "publish": [
        "github"
      ]
    },
    "mac": {
      "target": "dmg",
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": "AppImage",
      "publish": [
        "github"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "alphaleadership",
        "repo": "youtube-public",
        "releaseType": "draft"
      }
    ]
  }
}
